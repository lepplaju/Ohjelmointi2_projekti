<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="267.0" prefWidth="439.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Kayttoliittyma.KayttoliittymaHTGUIController">
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <bottom>
      <Button fx:id="Button_Vertailu" mnemonicParsing="false" onAction="#AvaaVertailu" text="Vertaile päivämäärän mukaan" BorderPane.alignment="CENTER">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </bottom>
   <right>
      <Button fx:id="Button_Ravinto" mnemonicParsing="false" onAction="#AvaaRuokailu" text="Ravintokirja" BorderPane.alignment="CENTER">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </right>
   <left>
      <Button fx:id="Button_Liikunta" mnemonicParsing="false" onAction="#AvaaLiikunta" text="Liikuntakirja" BorderPane.alignment="CENTER">
         <font>
            <Font size="20.0" />
         </font>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </Button>
   </left>
   <top>
      <Label text="Valitse mitä haluat kirjata" BorderPane.alignment="CENTER">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
   </top>
   <padding>
      <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
   </padding>
</BorderPane>
